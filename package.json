    "deepmerge": "4.3.1",
    "discord-rpc": "4.0.1",
    "electron-dl": "3.5.1",
    "electron-store": "8.1.0",
    "emoji-mart": "5.5.2",
    "fomantic-ui": "2.9.3",
    "hotkeys-js": "3.12.0",
    "i18n": "0.15.1",
    "i18n-iso-countries": "7.7.0",
    "jquery": "3.7.1",
    "music-metadata-browser": "2.5.10",
